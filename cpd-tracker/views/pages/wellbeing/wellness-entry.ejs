<div id="wellness-entry" v-cloak>
  <div class="container">
    <h1>Add a New Entry:</h1>
    <hr/>
    <ajax-form action="wellnessEntry" :syncing.sync="syncing" :cloud-error.sync="cloudError" @submitted="submittedForm()" :form-data="formData">
      <div class="col-sm-6">
        <div class="form-group">
          <label for="full-name">1 - 10 rate this week</label>
          <input class="form-control" id="slider1" type="range" min="1" max="10" step="1" v-model="formData.wellnessRating"/>
        </div>
      </div>
      <div class="row">
        <div class="col-sm-6">
          <div class="form-group text-right">
            <a class="btn btn-outline-info" href="/welcome">Cancel</a>
            <ajax-button type="submit" :syncing="syncing" class="btn btn-dark">Add Entry</ajax-button>
          </div>
        </div>
      </div>
    </ajax-form>
    <input type="button" value="test" v-on:click="testValues()" />
  </div>
</div>
<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>
